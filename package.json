  "scripts": {
    "tsc": "tsc --init",
    "check-types": "tsc",
    "start": "node dist/index.js",
    "dev:client": "parcel watch index.html --out-dir dist/client",
    "dev:server": "nodemon --watch ./server --exec babel-node ./server/index.js",
    "build:client": "parcel build index.html --out-dir dist/client",
    "build:server": "babel server -d dist",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "build": "yarn build:client && yarn build:server",
    "build:start": "yarn build && yarn start",
    "dev": "yarn dev:server & yarn dev:client",
    "deploy": "yarn build && gcloud app deploy --verbosity=info",
    "browse": "gcloud app browse",
    "logs": "gcloud app logs tail -s default",
    "test": "repo-tools test app -- dist/index.js"
  },
  "license": "MIT",
